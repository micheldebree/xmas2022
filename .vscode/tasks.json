	"version": "2.0.0",
	"tasks": [
		{
			"label": "run",
			"type": "shell",
			"command": "make ${fileWorkspaceFolder}/${fileBasenameNoExtension}.run",
			"problemMatcher": [],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "debug",
			"type": "shell",
			"command": "make ${fileWorkspaceFolder}/${fileBasenameNoExtension}.debug",
			"problemMatcher": [],
			"group": {
				"kind": "test",
				"isDefault": true
			}
		}
	]
}